//SORTING USING STL
#include<iostream>
#include<algorithm>
using namespace std;
int main()
{
  int a[50],n,i;
  cout<<"Enter the size of array: ";
  cin>>n;
  cout<<"Enter array:  ";
  for(i=0;i<n;i++)
  cin>>a[i];
  sort(a,a+n);
  cout<<"After sorting: ";
  for(i=0;i<n;i++)
  cout<<" "<<a[i];
  return 0;
}
