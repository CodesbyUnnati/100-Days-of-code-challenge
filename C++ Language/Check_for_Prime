#include<iostream>
using namespace std;
int main()
{
  int n,i;
  bool isprime=true;
  cout<<"Enter the integer: "<<endl;
  cin>>n;
  for(i=2;i<n/2;++i)
  {
      if(n%i==0)
    {
      isprime=false;
      break;
    }
  }
    if(isprime)
    cout<<"The Number is Prime Number.";
    else
    cout<<"Not Prime.";
    return 0;
  
}
