//TO CONVERT TO UPPER CASE
#include<stdio.h>
#include<string.h>
int main()
{
  char ch[100];
  int i;
  printf("Enter your String : ");
    scanf("%s",ch);
  for(i=0;i<=strlen(ch);i++)
  {

  if(ch[i]>=97&&ch[i]<122)
  
    ch[i]=ch[i]-32;
    
  }
  printf("Upper case letters : %s",ch);
  
  return 0;
}
