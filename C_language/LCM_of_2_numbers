//PROGRAM TO FIND LCM OF 2 NUMBERS..
#include<stdio.h>
int main()
{
  int min,n1,n2;
  printf("Enter 2 positive integers:  ");
  scanf("%d %d",&n1,&n2);
  min = (n1 > n2) ? n1 : n2;
  while(1)
  {
    if(min % n1 == 0 && min % n2 == 0)
    {
    printf("LCM of %d and %d is %d",n1,n2,min);
    break;
    }
  ++min;
  }
  return 0;
  }
